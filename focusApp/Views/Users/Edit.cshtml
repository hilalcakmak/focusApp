@model User
<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="UserId" />
    <label>Kullanıcı Adı:</label>
    <input asp-for="Username" /><br />
    <label>Email:</label>
    <input asp-for="Email" /><br />
    <label>Şifre:</label>
    <input asp-for="PasswordHash" /><br />
    <button type="submit">Kaydet</button>
</form>
